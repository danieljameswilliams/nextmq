{
  "name": "nextmq",
  "version": "0.1.0",
  "description": "A tiny queue + actions runtime for Next.js (CustomEvent + Context + code-splitting friendly).",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "prebuild": "npm run test",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run test && npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "next": ">=14"
  },
  "devDependencies": {
    "tsup": "8.5.1",
    "@types/node": "24.10.1",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "vitest": "^2.1.8",
    "@testing-library/react": "^16.1.0",
    "jsdom": "^25.0.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "nextjs",
    "queue",
    "events",
    "messages",
    "CustomEvent",
    "EventEmitter",
    "bullmq",
    "rabbitmq",
    "actions"
  ],
  "license": "MIT"
}